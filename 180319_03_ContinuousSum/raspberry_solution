//runtime error

#include<iostream>
#include<algorithm>
using namespace std;

int main() {
	int n=0;	//입력받을 정수 갯수(1<=n<=100,000)
	int A[100000] = { 0,};
	int Sum=0;
	long int D[1000][1000] = { 0, };
	cin >> n;
	for (int i = 1; i <= n; i++)
		cin >> A[i];

	D[1][1] = A[1];
	
	for (int j = 2; j <= n; j++) {
		D[1][j] = D[1][j - 1] + A[j];
		Sum = max(D[1][j], D[1][j - 1]);
		for (int k = j; k <= n; k++) {
			D[j][k] = D[j-1][k] - A[j-1];
			Sum = max(D[j][k], D[j][k - 1]);
		}	
	}
	cout << Sum;
}
