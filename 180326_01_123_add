/** 1, 2, 3 adding algorithm **/
#include <stdio.h>
int main()
{ 
	int T; 
	int dp[11]; 
	int n[100]; // n[11]도 가능, 일부러 크게 잡아놓음 
	scanf("%d", &T); 
	dp[0] = 1, dp[1] = 2, dp[2] = 4; 
	for(int i = 3; i < 11; i++) 
	{  
		dp[i] = dp[i - 3] + dp[i - 2] + dp[i - 1]; 
	} 
	for(int i = 0; i < T; i++) 
	{  
		scanf("%d", &n[i]); 
	} 
	for(int i = 0; i < T; i++) 
	{  
		printf("%d\n", dp[n[i] - 1]); 
	} 
	return 0;
}
